#include<stdio.h>
int main()
{
    float a,b,i,c;
    b=0;
    c=0;
    for(i=0; i<6; i++)
    {
        scanf("%f",&a);
        {
            if(a>0)
            {
                b++;
            }
            else if(a<0)
            {
                c++;
            }
        }
    }
    printf("%d valores positivos\n",(int)b);
    return 0;
}

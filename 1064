#include<stdio.h>
int main()
{
    float a,b,i,c;
    b=0;
    c=0;
    for(i=0; i<6; i++)
    {
        scanf("%f",&a);
        {
            if(a>0)
            {
            c+=a;
            b++;
            }

        }
    }
    printf("%d valores positivos\n",(int)b);
    printf("%.1f\n",c/b);
    return 0;
}


#include<stdio.h>
int main()

{
    int a,b,c,x,y,z;
    scanf("%d %d %d",&a,&b,&c);
    if(a>b && a>c)
    {
        z=a;
        if(b>c)
        {
            y=b;
            x=c;
        }
        else
        {
            y=c;
            x=b;
        }
    }
    else if(b>a && b>c)
    {
        z=b;
        if(a>c)
        {
            y=a;
            x=c;
        }
        else
        {
            y=c;
            x=a;
        }
    }

    else if(c>a && c>b)
    {
        z=c;
        if(a>b)
        {
            y=a;
            x=b;
        }
        else
        {
            y=b;
            x=a;
        }
    }

    printf("%d\n%d\n%d\n",x,y,z);
    printf("\n%d\n%d\n%d\n",a,b,c);
    return 0;

}

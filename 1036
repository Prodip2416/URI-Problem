#include<stdio.h>
#include<math.h>
int main()
{
   double a,b,c,n,x1,x2;
    scanf("%lf %lf %lf",&a,&b,&c);

    n=(pow(b,2)-(4*a*c));
    x1=(-b+sqrt(n))/(2*a);
    x2=(-b-sqrt(n))/(2*a);

    if(a!=0 && b>0)
    {
        printf("R1 = %.5lf\nR2 = %.5lf\n",x1,x2);
    }
    else
    {
        printf("Impossivel calcular\n");
    }
    return 0;

}

